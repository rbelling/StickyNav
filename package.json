{
  "scripts": {
    "start": "npm-run-all --parallel launchFe lint:watch",
    "launchFe": "babel-node ./tools/launch",
    "lint": "esw webpack.config.* src tools --color",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "BABEL_ENV=production npm run clean-dist && npm run lint",
    "build": "BABEL_ENV=production babel-node ./tools/build.js"
  },
  "name": "webpack-prototypes",
  "version": "1.0.0",
  "description": "Webpack-based boilerplate for quick prototypes",
  "repository": {
    "type": "git"
  },
  "license": "GPL-3.0",
  "browser": {},
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel-core": "6.14.0",
    "babel-loader": "6.2.5",
    "babel-plugin-remove-comments": "^2.0.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-babili": "0.0.8",
    "babel-preset-latest": "6.14.0",
    "babel-register": "6.14.0",
    "browser-sync": "2.14.0",
    "bs-fullscreen-message": "^1.1.0",
    "chalk": "1.1.3",
    "classlist-polyfill": "^1.0.3",
    "clean-css": "^3.4.20",
    "cross-env": "2.0.1",
    "css-loader": "0.24.0",
    "eslint": "3.4.0",
    "eslint-plugin-import": "1.14.0",
    "eslint-watch": "2.1.14",
    "extract-text-webpack-plugin": "1.0.1",
    "fastclick": "^1.0.6",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "img-loader": "^1.3.1",
    "json-loader": "^0.5.4",
    "lodash": "^4.16.6",
    "node-sass": "^3.10.1",
    "postcss-loader": "0.11.0",
    "pug": "^2.0.0-beta6",
    "pug-loader": "^2.3.0",
    "resolve-url-loader": "^1.6.0",
    "sass-loader": "4.0.0",
    "sass-resources-loader": "1.1.0",
    "strip-ansi": "^3.0.1",
    "style-ext-html-webpack-plugin": "^2.0.5",
